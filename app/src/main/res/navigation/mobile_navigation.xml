        <action
            android:id="@+id/action_nav_logout_fragment_to_nav_home_fragment"
            app:destination="@id/nav_home_fragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_work_manager_exhibit_fragment"
        android:name="com.example.gameban.fragment.WorkManagerExhibitFragment"
        android:label="GameBan"
        tools:layout="@layout/fragment_work_manager_exhibit">
        <action
            android:id="@+id/action_nav_work_manager_exhibit_fragment_to_nav_home_fragment"
            app:destination="@id/nav_home_fragment" />
    </fragment>

</navigation>