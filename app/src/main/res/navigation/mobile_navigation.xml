            android:id="@+id/action_nav_map_fragment_to_nav_home_fragment"
            app:destination="@id/nav_home_fragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_logout_fragment"
        android:name="com.example.gameban.fragment.LogoutFragment"
        android:label="GameBan"
